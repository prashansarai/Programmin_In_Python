//iterative Approach

def fact(n):
  if n<0:
    print("invalid")
  elif n==0 or n==1:
    print(1)
  else:
    i=n
    fact = 1
    while (i>1):
      fact = fact*i
      i-=1
    print(fact)

n = int(input())
fact(n)




//Recursive Approach

def fact(n):
  return 1 if n==1 or n==0 else n*(fact(n-1))
  
 n = int(input())
fact(n)
